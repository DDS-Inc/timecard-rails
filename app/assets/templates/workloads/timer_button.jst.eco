<% if @issue.get('is_running') is true: %>
  <a href="#" class="btn btn-warning timer-button--stop">
    <i class="fa fa-pause" title="stop"></i>
    STOP
  </a>
<% else: %>
  <a href="#" class="btn btn-primary timer-button--start">
    <i class="fa fa-play" title="start"></i>
    START
  </a>
<% end %>
